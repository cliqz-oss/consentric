{
    "name": "Cliqz Privacy Warning",
    "version": "1.0",
    "description": "Cliqz Privacy Warning",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "scripts": ["background.js"]
    }
  }
