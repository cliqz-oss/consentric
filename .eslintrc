{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "webextensions": true
  },
  "rules": {
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["src/stories/**/*.js"]}],
    "react/forbid-prop-types": 0,
    "arrow-body-style": 0
  }
}
